#include <stdio.h>
int main() 
{
    int arr[] = {1,9,97,22,11,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int even_count=0,odd_count=0;
    for(int i=0;i<n;i++) 
    {
        if(arr[i]%2==0) 
        {
            even_count++;
        } 
        else 
        {
            odd_count++;
        }
    }
    printf("Even elements:%d\n",even_count);
    printf("Odd elements:%d\n",odd_count);
    return 0;
}
